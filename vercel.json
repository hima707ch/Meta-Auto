{
  "version": 2,
  "builds": [
    { "src": "server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/.*", "dest": "/server.js" }
  ],
  "crons": [
    {
      "path": "/api/post-to-facebook",
      "schedule": "0 8,12,16,20 * * *"
    },
    {
      "path": "/api/post-youtube-video",
      "schedule": "0 8,25 19 * * *"
    }
  ]
}
