{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "crons": [
    {
      "path": "/api/masterclass",
      "schedule": "0 8,12,16,20 * * *"
    }
  ]
  }